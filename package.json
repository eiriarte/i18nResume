{
  "name": "i18nresume",
  "version": "1.0.0",
  "description": "React component to render json-resume data in different languages.",
  "author": "Eduardo Iriarte Gahete",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "CV",
    "resume",
    "json-resume",
    "React",
    "i18n"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eiriarte/i18nresume"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/i18nresume.umd.cjs",
  "module": "./dist/i18nresume.js",
  "browser": "./dist/i18nresume.js",
  "exports": {
    ".": {
      "import": "./dist/i18nresume.js",
      "require": "./dist/i18nresume.umd.cjs"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prepare": "npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/material": "^5.15.19",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/node": "^20.13.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  }
}
